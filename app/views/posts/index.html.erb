<div class="posts-index">
<% @posts.each do |post| %>
<%= link_to post do %>
<div class="col-md-4">
  <ul>
    <li>タイトル：<%= post.title %></li>
    <li>投稿日：<%= post.created_at.strftime("%Y年%m月%d日") %></li>
  </ul>
  </div>
  <% end %>
<% end %>
<%= link_to '新規作成', new_post_path, class: "btn btn-primary" %>
</div>
